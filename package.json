{
  "name": "botmate",
  "version": "0.0.1-alpha.16",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --projects=server,web",
    "dev": "cross-env NODE_ENV=dev nx run-many --target=dev --projects=server,web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "codegen": "graphql-codegen --config codegen.yaml",
    "gen": "plop",
    "release": "release-it",
    "gulp": "gulp"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "@release-it/conventional-changelog": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint-config-custom": "workspace:*",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "nx": "15.8.5",
    "plop": "^3.1.2",
    "prettier": "latest",
    "release-it": "^15.7.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.17.1"
}
